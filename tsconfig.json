{
  "compilerOptions": {
    "declaration": false,
    "noImplicitAny": true,
    "noEmitOnError": true,
    "noUnusedLocals": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "jsx": "react",
    "target": "es2016",
    "lib": ["es5", "es2015.collection", "es2015.promise", "dom", "es2016", "es2017"],
    "baseUrl": ".",
    "outDir": "./build/out",
    "skipLibCheck": true
  },
 "include": ["src"],
 "exclude": ["node_modules"]
}
