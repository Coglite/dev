{
  "name": "coglite",
  "productName": "Coglite Desktop",
  "main": "build/desktop/desktop.js",
  "version": "0.0.1",
  "description": "app",
  "license": "MIT",
  "private": true,
  "author": "Coglite team",
  "repository": {
    "type": "git",
    "url": "https://github.com/coglite/coglite-desktop"
  },
  "homepage": "https://github.com",
  "scripts": {
    "start": "node fuse.js",
    "clean": "rimraf .fusebox out dist coverage",
    "updatedeps": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@blueprintjs/core": "2.3.1",
    "@blueprintjs/datetime": "2.0.3",
    "@blueprintjs/icons": "^2.2.1",
    "@blueprintjs/select": "2.0.1",
    "@blueprintjs/table": "2.1.1",
    "csstips": "0.0.3",
    "csx": "9.0.0",
    "dotenv": "5.0.1",
    "history": "4.7.2",
    "less": "3.0.4",
    "lodash": "4.17.10",
    "mobx": "4.3.0",
    "mobx-react": "5.1.2",
    "mobx-react-router": "4.0.4",
    "mobx-rest": "2.2.5",
    "mobx-rest-fetch-adapter": "0.1.4",
    "mobx-utils": "4.0.1",
    "monolite": "0.4.6",
    "prop-types": "15.6.1",
    "react": "16.4.0",
    "react-dom": "16.4.0",
    "react-icons": "2.2.7",
    "react-redux": "5.0.7",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "react-test-renderer": "16.4.0",
    "react-tooltip": "3.6.0",
    "react-transition-group": "2.3.1",
    "react-virtualized": "9.19.1",
    "react-virtualized-select": "3.1.3",
    "redux": "4.0.0",
    "redux-devtools": "3.4.1",
    "redux-devtools-dock-monitor": "1.1.3",
    "redux-devtools-log-monitor": "1.4.0",
    "redux-thunk": "2.2.0",
    "reflect-metadata": "0.1.12",
    "returnof": "1.1.1",
    "rmwc": "1.6.3",
    "shelljs": "0.8.2",
    "source-map-support": "0.5.6",
    "storm-react-diagrams": "5.2.1",
    "tslib": "1.9.1",
    "tween.js": "16.6.0",
    "typestyle": "2.0.1",
    "when-switch": "0.3.1",
        "cesium": "^1.43.0",
    "classnames": "^2.2.5",
    "d3-fetch": "^1.1.0",
    "deep-equal": "^1.0.1",
    "deep-object-diff": "^1.1.0",
    "electron-log": "2.2.14",
    "oboe": "2.1.4",
    "react-ace": "6.1.1",
    "react-color": "2.14.1",
    "react-linkify": "0.2.2",
    "redux-logger": "^3.0.6",
    "reselect": "3.0.1"
  },
  "devDependencies": {
    "cesium-typings": "^1.40.9",
    "@types/classnames": "2.2.3",
    "@types/electron-devtools-installer": "2.0.3",
    "@types/history": "4.6.2",
    "@types/lodash": "4.14.107",
    "@types/node": "10.1.2",
    "@types/prop-types": "15.5.2",
    "@types/react": "16.3.14",
    "@types/react-dom": "16.0.5",
    "@types/react-loadable": "5.4.0",
    "@types/react-redux": "6.0.0",
    "@types/react-router": "4.0.25",
    "@types/react-virtualized-select": "3.0.5",
    "@types/redux-devtools": "3.0.44",
    "@types/redux-devtools-dock-monitor": "1.1.32",
    "@types/redux-devtools-log-monitor": "1.0.33",
    "@types/d3-fetch": "^1.0.1",
    "@types/deep-equal": "0.0.30",
    "@types/dom4": "^1.5.20",
    "@types/geojson": "^1.0.6",
    "@types/oboe": "^2.0.28",
    "@types/openlayers": "^3.20.6",
    "@types/proj4": "^2.3.4",
    "@types/react-color": "2.13.5",
    "@types/redux-logger": "^3.0.5",
    "cross-env": "5.1.4",
    "cssstats": "3.2.0",
    "electron": "2.0.2",
    "electron-builder": "20.15.1",
    "electron-devtools-installer": "2.2.4",
    "fuse-box": "3.2.2",
    "less": "3.0.4",
    "less-plugin-glob": "2.0.2",
    "node-sass": "4.9.0",
    "ts-node": "6.0.5",
    "tslint": "5.9.1",
    "typescript": "2.8.3",
    "uglify-es": "3.3.9"
  },
  "build": {
    "appId": "com.example.coglite",
    "files": [
      "!src",
      "!docs",
      "!tsconfig.json",
      "!fuse.js",
      "!dev-app-update.yml",
      "!README.md",
      "!LICENSE",
      "!globals.d.ts"
    ],
    "linux": {
      "target": [
        "tar.gz"
      ]
    },
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "win": {
      "target": [
        "zip",
        "nsis"
      ]
    },
    "publish": {
      "owner": "coglite",
      "provider": "github",
      "repo": "coglite-desktop"
    }
  },
  "jest": {
    "setupFiles": [
      "./test/setup.ts"
    ],
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/mock-file.ts",
      "\\.(css|less)$": "<rootDir>/test/mock-style.ts"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "coveragePathIgnorePatterns": [
      "./node_modules",
      "./out",
      "./build",
      "./dist",
      "./test",
      "./docs",
      "\\.story.tsx$"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverage": false,
    "mapCoverage": true
  }
}
