    "qrcode": "1.2.0",
    "react": "^16.2.0",
    "react-codemirror2": "3.0.7",
    "react-dom": "^16.2.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "semantic-ui-react": "0.78.3",
    "sql-formatter": "2.1.0",
    "tslib": "^1.9.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.40",
    "@babel/core": "^7.0.0-beta.40",
