{
  "name": "coglite",
  "productName": "Coglite Desktop",
  "main": "dist/desktop/desktop.js",
  "version": "0.0.1",
  "description": "app",
  "license": "MIT",
  "private": true,
  "author": "Coglite team",
  "repository": {
    "type": "git",
    "url": "https://github.com/coglite/coglite-desktop"
  },
  "homepage": "https://github.com",
  "scripts": {
    "start": "node fuse",
    "build-storybook": "build-storybook",
    "clean": "rimraf .fusebox out dist coverage",
    "compile": "cross-env NODE_ENV=production ts-node fuse",
    "coverage:upload": "cat ./coverage/lcov.info | coveralls",
    "coverage": "yarn test --coverage --no-cache",
    "dist": "electron-builder --mac --win --linux --ia32 --x64",
    "draft": "electron-builder -p always --draft --mac --win --linux --ia32 --x64",
    "format": "prettier --write **/*.ts && tslint -p . --fix",
    "lint": "tslint --project .",
    "pack": "electron-builder --dir",
    "updatedeps": "electron-builder install-app-deps",
    "precommit": "lint-staged",
    "prepush": "npm-run-all",
    "sanity": "npm-run-all clean typecheck lint test clean compile clean",
    "start1": "ts-node fuse",
    "storybook": "start-storybook -p 6006",
    "test": "jest",
    "typecheck": "tsc -p . --noEmit --pretty"
  },
  "dependencies": {
    "codemirror": "5.38.0",
    "csx": "9.0.0",
    "dotenv": "5.0.1",
    "dragula": "3.7.2",
    "electron-devtools-installer": "2.2.4",
    "history": "4.7.2",
    "less": "3.0.4",
    "lodash": "4.17.10",
    "material-ui": "1.0.0-beta.42",
    "material-ui-icons": "1.0.0-beta.36",
    "material-ui-image": "3.0.0-pre.6",
    "mobx": "4.3.0",
    "mobx-react": "5.1.2",
    "mobx-react-router": "4.0.2",
    "mobx-rest": "2.2.5",
    "mobx-rest-fetch-adapter": "0.1.4",
    "mobx-utils": "4.0.1",
    "mousetrap": "1.6.1",
    "popmotion": "8.1.22",
    "popmotion-react": "2.1.1",
    "prop-types": "15.6.1",
    "purecss": "1.0.0",
    "ramda": "0.25.0",
    "react": "16.3.2",
    "react-codemirror": "1.0.0",
    "react-dom": "16.3.2",
    "react-icons": "2.2.7",
    "react-jss": "8.4.0",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "react-select": "1.2.1",
    "react-tooltip": "3.6.0",
    "react-transition-group": "2.3.1",
    "react-virtualized": "9.19.1",
    "react-virtualized-select": "3.1.3",
    "reflect-metadata": "0.1.12",
    "rmwc": "1.6.3",
    "storm-react-diagrams": "5.2.1",
    "tslib": "1.9.1",
    "tween.js": "16.6.0",
    "typestyle": "2.0.1",
    "react-dragula": "1.1.17",
    "react-powerplug": "0.1.5",
    "react-test-renderer": "16.4.0",
    "shelljs": "0.8.2",
    "js-yaml": "3.11.0",
    "jss": "9.8.1",
    "jss-compose": "5.0.0",
    "jss-nested": "6.0.1",
    "react-codemirror2": "5.0.2",
    "source-map-support": "0.5.6"
  },
  "devDependencies": {
    "@types/js-yaml": "3.11.1",
    "@material-ui/icons": "1.0.0-beta.42",
    "@types/classnames": "2.2.3",
    "@types/dragula": "2.1.33",
    "@types/history": "4.6.2",
    "@types/jest": "22.2.3",
    "@types/lodash": "4.14.107",
    "@types/mousetrap": "1.6.0",
    "@types/node": "10.1.2",
    "@types/prop-types": "15.5.2",
    "@types/react": "16.3.14",
    "@types/react-codemirror": "1.0.1",
    "@types/react-dom": "16.0.5",
    "@types/react-loadable": "5.4.0",
    "@types/react-router": "4.0.25",
    "@types/react-select": "1.2.6",
    "@types/react-test-renderer": "16.0.1",
    "@types/react-virtualized-select": "3.0.5",
    "babel-preset-react": "6.24.1",
    "cross-env": "5.1.4",
    "cssstats": "3.2.0",
    "csstips": "0.2.3",
    "less": "3.0.4",
    "electron": "2.0.2",
    "electron-builder": "20.14.7",
    "fuse-box": "3.2.2",
    "gulp": "3.9.1",
    "gulp-concat": "2.6.1",
    "gulp-postcss": "7.0.1",
    "gulp-sass": "4.0.1",
    "gulp-sourcemaps": "2.6.4",
    "jest": "22.4.3",
    "jest-expect-contain-deep": "1.0.1",
    "less-plugin-glob": "2.0.2",
    "merge-stream": "1.0.1",
    "minimist": "1.2.0",
    "node-notifier": "5.2.1",
    "node-sass": "4.9.0",
    "postcss": "6.0.21",
    "postcss-cli": "5.0.0",
    "postcss-cssnext": "3.1.0",
    "postcss-csso": "3.0.0",
    "postcss-discard-duplicates": "2.1.0",
    "postcss-easy-import": "3.0.0",
    "postcss-import": "11.1.0",
    "postcss-inline-svg": "3.1.1",
    "postcss-svgo": "2.1.6",
    "postcss-url": "7.3.2",
    "ts-jest": "22.4.3",
    "ts-node": "6.0.5",
    "tslint": "5.9.1",
    "typescript": "2.8.3"
  },
  "build": {
    "appId": "com.example.coglite",
    "files": [
      "!src",
      "!docs",
      "!tsconfig.json",
      "!fuse.js",
      "!dev-app-update.yml",
      "!README.md",
      "!LICENSE",
      "!globals.d.ts"
    ],
    "linux": {
      "target": [
        "tar.gz"
      ]
    },
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "win": {
      "target": [
        "zip",
        "nsis"
      ]
    },
    "publish": {
      "owner": "coglite",
      "provider": "github",
      "repo": "coglite-desktop"
    }
  },
  "jest": {
    "setupFiles": [
      "./test/setup.ts"
    ],
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/mock-file.ts",
      "\\.(css|less)$": "<rootDir>/test/mock-style.ts"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "coveragePathIgnorePatterns": [
      "./node_modules",
      "./out",
      "./build",
      "./dist",
      "./test",
      "./docs",
      "\\.story.tsx$"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverage": false,
    "mapCoverage": true
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ]
  }
}
