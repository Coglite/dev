    "mobx": "3.6.1",
    "mobx-react": "4.4.2",
    "mobx-react-router": "^4.0.1",
    "mousetrap": "^1.6.1",
    "popmotion": "^8.1.17",
    "popmotion-react": "^2.1.0",
    "qrcode": "1.2.0",
    "react": "16.2.0",
    "react-codemirror2": "3.0.7",
    "react-dom": "16.2.0",
    "react-jss": "^8.3.3",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "semantic-ui-react": "0.78.3",
    "sql-formatter": "2.1.0",
    "ts-loader": "4.0.1",
    "tslib": "1.9.0"
  },
