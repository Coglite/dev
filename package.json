{
  "name": "react-slate",
  "version": "0.1.0",
  "description": "Interactive playground for your React JS prototypes and experiments",
  "license": "MIT",
  "scripts": {
    "prebuild": "npm run clean",
    "start": "webpack-dev-server --inline --hot",
    "build": "cross-env NODE_ENV=production webpack",
    "clean": "rimraf dist",
    "start2": "cross-env NODE_ENV=development webpack-dev-server -d --inline --hot --progress --no-info --config=webpack.config.js"
  },
  "dependencies": {
    "@blueprintjs/core": "1.35.7",
    "bluebird": "^3.4.6",
    "body-parser": "^1.15.1",
    "brace": "^0.11.1",
    "classnames": "^2.2.5",
    "codemirror": "^5.20.2",
    "express": "^4.14.0",
    "flexboxgrid": "^6.3.1",
    "fs-extra": "^1.0.0",
    "history": "4.7.2",
    "minimist": "^1.2.0",
    "mobx": "4.1.0",
    "mobx-react": "5.0.0",
    "npm-programmatic": "0.0.6",
    "npmi": "^2.0.1",
    "react": "16.2.0",
    "react-ace": "5.9.0",
    "react-addons-css-transition-group": "15.6.2",
    "react-addons-test-utils": "15.6.2",
    "react-dnd": "2.5.4",
    "react-dnd-html5-backend": "2.5.4",
    "react-dom": "16.2.0",
    "react-flexbox-grid": "^0.10.2",
    "react-router": "^2.5.2",
    "recast": "^0.11.17",
    "server-destroy": "^1.0.1",
    "socket.io": "^1.7.2",
    "socket.io-client": "^1.5.1",
    "superagent": "3.8.2"
  },
  "devDependencies": {
    "@types/node": "^9.4.7",
    "@types/react": "16.0.40",
    "@types/react-dom": "16.0.4",
    "ast-traverse": "^0.1.1",
    "autoprefixer": "^8.1.0",
    "awesome-typescript-loader": "^4.0.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "commitizen": "^2.9.6",
    "connect-history-api-fallback": "^1.5.0",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "cz-conventional-changelog": "^2.1.0",
    "enzyme": "^3.3.0",
    "eslint": "^4.19.0",
    "esprima": "^4.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.0.7",
    "imports-loader": "^0.8.0",
    "jest-cli": "^22.4.2",
    "json-loader": "^0.5.7",
    "node-sass": "^4.7.2",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.7",
    "source-map-support": "^0.5.4",
    "style-loader": "^0.20.3",
    "stylelint": "^9.1.3",
    "stylelint-webpack-plugin": "^0.10.3",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2",
    "url-loader": "^1.0.1",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.12",
    "webpack-dashboard": "^1.1.1",
    "webpack-dev-middleware": "^3.0.1",
    "webpack-dev-server": "^3.1.1",
    "webpack-hot-middleware": "^2.21.2",
    "webpack-split-by-path": "^2.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "scriptPreprocessor": "<rootDir>/test/preprocessor.js",
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$",
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/mocks/fileMock.js",
      "^.+\\.(css|less)$": "<rootDir>/test/mocks/styleMock.js"
    },
    "coverageDirectory": "<rootDir>/coverage",
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/node_modules/**",
      "!**/typings/**",
      "!**/__tests__/**"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    }
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  }
}
