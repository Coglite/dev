{
  "name": "coglite",
  "productName": "Coglite Desktop",
  "description": "Coglite Desktop",
  "version": "0.0.0",
  "private": true,
  "author": "Coglite team",
  "copyright": "Â© 2017, Coglite inc.",
  "main": "dist/desktop.js",
  "build": {
    "appId": "com.example.coglite",
    "files": [
      "tasks/html/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "tasks/resources"
    },
    "publish": null
  },
  "scripts": {
    "start": "node tasks/start.js",
    "desktop.dev": "cross-env ELECTRON_DEV_URL=http://localhost:3000 electron .",
    "updatedeps": "electron-builder install-app-deps",
    "preunit": "webpack --config=tasks/webpack.unit.config.js --env=test --display=none",
    "unit": "electron-mocha temp/specs.js --renderer --require source-map-support/register",
    "test": "npm run unit",
    "release": "npm test && webpack --config=tasks/webpack.app.config.js --env=production && electron-builder"
  },
  "dependencies": {
    "codemirror": "5.36.0",
    "electron-devtools-installer": "2.2.3",
    "event-kit": "^2.4.0",
    "execa": "0.10.0",
    "fs-jetpack": "1.3.0",
    "glamorous": "^4.12.1",
    "mobx": "4.1.1",
    "mobx-react": "5.0.0",
    "qrcode": "1.2.0",
    "react": "16.3.1",
    "react-codemirror2": "4.2.1",
    "react-dom": "16.3.1",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "semantic-ui-react": "0.79.1",
    "sql-formatter": "2.2.0",
    "ts-loader": "4.1.0",
    "tslib": "1.9.0"
  },
  "devDependencies": {
    "@types/codemirror": "0.0.56",
    "@types/execa": "0.9.0",
    "@types/jest": "22.2.2",
    "@types/node": "9.6.2",
    "@types/react": "16.3.5",
    "@types/react-dom": "16.0.4",
    "@types/react-router": "4.0.23",
    "@types/react-router-dom": "4.2.6",
    "@types/semantic-ui": "2.2.6",
    "chai": "4.1.2",
    "chalk": "2.3.2",
    "clean-webpack-plugin": "0.1.19",
    "copy-webpack-plugin": "4.5.1",
    "css-loader": "0.28.11",
    "electron": "1.8.4",
    "electron-builder": "20.8.1",
    "electron-mocha": "6.0.1",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "1.1.11",
    "friendly-errors-webpack-plugin": "1.6.1",
    "html-webpack-plugin": "3.2.0",
    "json-loader": "0.5.7",
    "less": "3.0.1",
    "less-loader": "4.1.0",
    "mocha": "5.0.5",
    "node-sass": "4.8.3",
    "raw-loader": "0.5.1",
    "sass-loader": "6.0.7",
    "semantic-ui-css": "2.3.1",
    "semantic-ui-less": "2.3.1",
    "source-map-support": "0.5.4",
    "style-loader": "0.20.3",
    "typescript": "2.8.1",
    "url-loader": "1.0.1",
    "webpack": "4.5.0",
    "webpack-cli": "2.0.14",
    "webpack-merge": "4.1.2",
    "webpack-node-externals": "1.7.2"
  }
}
