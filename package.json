{
  "name": "aiblocs",
  "author": "Boris Musarais",
  "version": "0.1.5",
  "description": "A user friendly interface for creating Machine Learning models.",
  "private": true,
  "main": "src/main.js",
  "scripts": {
    "start": "electron -r babel-register .",
    "pack": "electron-builder --dir",
    "pack-mac": "electron-builder --mac dir"
  },
  "dependencies": {
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-register": "6.26.0",
    "chart.js": "2.7.2",
    "cross-env": "5.1.4",
    "electron-google-analytics": "0.0.24",
    "express-open-in-editor": "3.1.1",
    "flow": "0.2.3",
    "jquery": "3.3.1",
    "load-json-file": "4.0.0",
    "open": "0.0.5",
    "open-in-editor": "2.2.0",
    "react": "16.3.1",
    "react-bootstrap-treeview": "0.1.0",
    "react-dom": "16.3.1",
    "react-scripts": "1.1.4",
    "react-treebeard": "2.1.0",
    "react-treeview": "0.4.7",
    "showdown": "1.8.6",
    "treeview-react-bootstrap": "0.4.6",
    "webpack": "4.5.0",
    "webpack-target-electron-renderer": "0.4.0"
  },
  "devDependencies": {
    "electron": "1.8.4",
    "electron-builder": "20.8.1",
    "electron-packager": "12.0.0"
  },
  "build": {
    "appId": "ai.blocks",
    "mac": {
      "category": "app.iablocks"
    },
    "files": [
      "node_modules/",
      "app.html",
      "main.prod.js",
      "main.prod.js.map",
      "build_scripts/",
      "examples/",
      "scripts/",
      "templates/",
      "src/",
      "package.json"
    ]
  }
}
