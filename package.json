{
  "name": "coglite",
  "productName": "Coglite Desktop",
  "description": "Coglite Desktop",
  "version": "0.0.0",
  "private": true,
  "author": "Coglite team",
  "copyright": "Â© 2017, Coglite inc.",
  "main": "dist/desktop.js",
  "build": {
    "appId": "com.example.coglite",
    "files": [
      "tasks/html/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "tasks/resources"
    },
    "publish": null
  },
  "scripts": {
    "updatedeps": "electron-builder install-app-deps",
    "preunit": "webpack --config=tasks/webpack.unit.config.js --env=test --display=none",
    "unit": "electron-mocha temp/specs.js --renderer --require source-map-support/register",
    "test": "npm run unit",
    "start": "ts-node tasks/start.ts",
    "release": "npm test && webpack --config=tasks/webpack.app.config.js --env=production && electron-builder"
  },
  "dependencies": {
    "fs-jetpack": "1.3.0",
    "mobx": "3.5.1",
    "mobx-react": "4.4.1",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.39",
    "@babel/core": "7.0.0-beta.39",
    "@babel/plugin-proposal-class-properties": "7.0.0-beta.39",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0-beta.39",
    "@babel/plugin-proposal-pipeline-operator": "7.0.0-beta.39",
    "@babel/plugin-syntax-object-rest-spread": "7.0.0-beta.39",
    "@babel/plugin-transform-modules-commonjs": "7.0.0-beta.39",
    "@babel/polyfill": "7.0.0-beta.39",
    "@babel/preset-env": "7.0.0-beta.39",
    "@babel/preset-react": "7.0.0-beta.39",
    "@babel/preset-stage-3": "7.0.0-beta.39",
    "@babel/preset-typescript": "7.0.0-beta.39",
    "@types/node": "9.4.5",
    "@types/react": "16.0.36",
    "@types/react-dom": "16.0.3",
    "@types/react-router": "4.0.21",
    "@types/react-router-dom": "4.2.3",
    "babel-loader": "8.0.0-beta.0",
    "babel-node": "^7.0.0-beta.3",
    "babel-plugin-module-resolver": "3.0.0",
    "babel-plugin-transform-object-rest-spread": "7.0.0-beta.3",
    "chai": "4.1.2",
    "chalk": "2.3.1",
    "copy-webpack-plugin": "4.4.1",
    "css-loader": "0.28.9",
    "electron": "1.8.2",
    "electron-builder": "19.56.0",
    "electron-mocha": "5.0.0",
    "file-loader": "1.1.6",
    "friendly-errors-webpack-plugin": "1.6.1",
    "html-webpack-plugin": "2.30.1",
    "less": "3.0.0",
    "less-loader": "4.0.5",
    "mocha": "5.0.0",
    "node-sass": "4.7.2",
    "raw-loader": "0.5.1",
    "sass-loader": "6.0.6",
    "source-map-support": "0.5.3",
    "style-loader": "0.20.1",
    "typescript": "2.7.1",
    "url-loader": "0.6.2",
    "webpack": "4.0.0-beta.1",
    "webpack-cli": "2.0.4",
    "webpack-merge": "4.1.1",
    "webpack-node-externals": "1.6.0"
  }
}
